<div class='resource-editor' style='border: none'>
  <div></div>
  <div class='inputsv2'>
    <div class='label'>Title</div>
    <input class='title gcs-input-30' style='width: 100%; text-align: left' value='{{title}}' type='text' />
    <div class='label'>Note</div>
    {{#if notes}}
      <textarea class='notes gcs-input-ta' style='width: 100%; height: 400px !important' rows='10'>{{notes}}</textarea>
    {{else}}
      <textarea
        class='markdown gcs-input-ta'
        style='width: 100%; height: 400px !important'
        rows='10'
      >{{{markdown}}}</textarea>
    {{/if}}
    <div class='label'>Page Ref</div>
    <input class='pageref gcs-input-sm5' type='text' value='{{pageref}}' />
    {{#if (defined save)}}
      <input class='save' type='checkbox' {{checked (eq save true)}} />
      <div class='label'>Check this to save this note during imports</div>
    {{/if}}
  </div>
</div>